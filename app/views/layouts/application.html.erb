  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <header>
    <nav>
      <div class="nav-wrapper light-blue lighten-2">
        <a href="/" class="brand-logo">Fundraise For Love</a>
        <ul id="nav-mobile" class="right hide-on-med-and-down">
